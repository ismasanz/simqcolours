#: colours/get_colour.tpl:80
msgid "Please rate the confidence you have in your answer"
msgstr "Indica la confianza que tienes en tu respuesta"

#: colours/get_colour.tpl:82
msgid "Not at all confident"
msgstr "Ninguna confianza"

#: colours/get_colour.tpl:82
